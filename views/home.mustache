<html lang="en">

<head>
	{{> heads}}
	<title>Welcome to Subs</title>
</head>

<body>
	<!--recieve //id title body owner from sqlRoute.js-->

	{{!-- add new event --}}
	<div id="addPanel">
		<form method="post" action="/record">
			{{> addPanel}}
			<button class="cta"><span class="material-icons-two-tone md-light">addchart</span> Create</button>
			<button type='button' class="altcta" id="closeAddPanel"><span class="material-icons-two-tone md-light">cancel</span> Cancel</button>
		</form>
	</div>

	{{!-- Page nav head --}}
	{{> navHeader}}


	{{^errReason}}
	<p>Welcome to Subs<br>
		Your test email is: <br> {{email}}
		<br>and username is @{{username}}
	</p>

	<p class="wtxt">Your Subscriptions</p>
	{{#subs}}
	{{./}}
	<div class="subsHolder">
		<h class="wtxt">{{currency}}{{cost}}</h><br>
		<h class="wtxt">in: </h><br>
		<h class="wtxt">{{name}}<br>
	</div>
	{{/subs}}




	<br>
	<a href="/logout"><button class="cta"><span class="material-icons-two-tone">logout</span> Logout</button></a>
	{{/errReason}}

</body>

</html>