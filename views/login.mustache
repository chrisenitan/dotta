<html>

<head>
	{{> heads}}
	<title>Welcome to Subs</title>
</head>

<body>
	{{> header}}
	<!--recieve //id title body owner from sqlRoute.js-->
	<div id="pageContainer">
		{{^errReason}}
		<p>Login to your account</p>
		<form method="post" action="account/login">
			<div class="TinputDiv">
				<label for="username">Username</label><br>
				<input type="text" value="EmptyNoise" name="username"><br>
				<h class="inputDis">Your username</h><br><br>
			</div>

			<div class="TinputDiv">
				<label for="password">Password</label><br>
				<input type="password" value="12342" name="password"><br>
				<h class="inputDis">Your password is always secure</h><br><br>
			</div>

			<input type="hidden" name="action" value="logIn">
			<button class="cta"><span class="material-icons-two-tone md-light">login</span> Login</button>
		</form>
		<br>
		<a href="account/recovery" class="smallTxt">Forgot Password?</a>
		{{/errReason}}

	</div>
</body>

</html>